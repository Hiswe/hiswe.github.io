{
  "name": "hexo-site",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": ">=14.15.5",
    "yarn": ">=1.22.10"
  },
  "hexo": {
    "version": "5.4.0"
  },
  "scripts": {
    "start": "yarn dev",
    "dev": "hexo clean && yarn build:dev && run-p dev:*",
    "dev:watch": "gulp dev",
    "dev:serve": "hexo server --draft",
    "build": "cross-env NODE_ENV=production gulp build",
    "build:dev": "gulp build",
    "build:icons": "gulp icons",
    "release": "hexo clean && yarn build && hexo generate --deploy"
  },
  "dependencies": {
    "cross-env": "^7.0.3",
    "hexo": "^5.4.0",
    "hexo-cli": "^4.2.0",
    "hexo-deployer-git": "3.0.0",
    "hexo-generator-archive": "^1.0.0",
    "hexo-generator-category": "^1.0.0",
    "hexo-generator-feed": "^3.0.0",
    "hexo-generator-index": "^2.0.0",
    "hexo-generator-tag": "^1.0.0",
    "hexo-renderer-ejs": "^1.0.0",
    "hexo-renderer-marked": "^4.0.0",
    "hexo-server": "^2.0.0",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@babel/core": "^7.13.16",
    "@babel/preset-env": "^7.13.15",
    "babel-loader": "^8.2.2",
    "browser-sync": "^2.26.14",
    "del": "^6.0.0",
    "fancy-log": "^1.3.3",
    "gulp": "^4.0.0",
    "gulp-cheerio": "^1.0.0",
    "gulp-clean-css": "^4.0.0",
    "gulp-if": "^3.0.0",
    "gulp-load-plugins": "^2.0.6",
    "gulp-plumber": "^1.2.1",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^4.1.0",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-svg-symbols": "^3.2.3",
    "gulp-util": "^3.0.7",
    "lazypipe": "^1.0.2",
    "npm-run-all": "^4.1.5",
    "parallelshell": "^3.0.2",
    "parcel-bundler": "^1.12.5",
    "yargs": "^16.2.0"
  },
  "description": "Hiswe blog",
  "main": "index.js",
